<% web_address ||= @web_address %>
<% content_for "web_address_#{web_address.id}_label".to_sym do %>
  <%=h web_address.type.value %>
<% end %>
<% content_for "web_address_#{web_address.id}_value".to_sym do %>
  <%= auto_link sanitize(web_address.address), :html => { :target => '_blank' } %>
<% end %>
<%= render :partial => 'users/components/entry', :locals => {:type => 'web_address', :entry => web_address} %>